{
	"$schema"    : "vscode://schemas/color-theme",
	"name"       : "Dark+ (default dark) (mblet)",
	"include"    : "./dark_plus.json",
	"tokenColors": [
        {
			"name" : "operator",
			"scope": [
                "mblet_operator",
                "mblet_doubleDoubleDot"
            ],
			"settings": {
                "foreground": "#d5b734",
                "fontStyle" : "bold"
			}
        },
        {
            "name" : "private_member",
			"scope": [
                "mblet_private_member"
            ],
			"settings": {
                "fontStyle": "italic"
			}
        }
    ]
}