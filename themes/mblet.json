{
	"$schema"    : "vscode://schemas/color-theme",
	"name"       : "Dark+ (default dark) (mblet)",
	"include"    : "../extentions.default/theme-defaults/themes/dark_plus.json",
	"tokenColors": [
        {
			"name"    : "std",
			"scope"   : "mblet_std",
			"settings": {
                "foreground": "#3bb196",
                "fontStyle" : "bold"
			}
        },
        {
			"name" : "operator",
			"scope": [
                "mblet_operator",
                "mblet_doubleDoubleDot"
            ],
			"settings": {
                "foreground": "#d5b734",
                "fontStyle" : "bold"
			}
        }
    ]
}